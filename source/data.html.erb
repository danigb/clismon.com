<% clismon_data.groups_by_name.values.each do |group| %>
  <h2><%= group.title %></h2>
  <% group.clips.each do |clip| %>
    <%= clip.id %> / <%= clip.name %> / <%= clip.locator %><br>
    <%= climage((clip.group_name ? clip.group_name : 'nil') + '/' +
      (clip.locator ? clip.locator : 'nil')) %><br>
  <% end %>
<% end %>
